# Пример файла переменных окружения
# Скопируйте этот файл в .env и заполните своими значениями

# App configuration
APP_NAME="FitMachine API"
APP_VERSION="1.0.0"
APP_PORT="8080"
APP_ENV="prod"

# Database configuration
DB_HOST="localhost"
DB_PORT="5432"
DB_USER="fitmachine_user"
DB_PASSWORD="fitmachine_password"
DB_NAME="fitmachine_local"
DB_SSLMODE="disable"

# Server configuration
SERVER_HOST="localhost"
SERVER_PORT="8080"
SERVER_READ_TIMEOUT="30"
SERVER_WRITE_TIMEOUT="30"

# Logging configuration
LOG_LEVEL="debug"
LOG_FORMAT="text"
